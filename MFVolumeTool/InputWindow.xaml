<Window x:Name="InputWindow1" x:Class="MFVolumeTool.InputWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MFVolumeTool"
        mc:Ignorable="d"
        Title="InputWindow" Height="175.924" Width="381.545" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <Grid Margin="10">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="3*"/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Button x:Name="BtnConfirm" Content="确认" Grid.Column="1" Margin="10,23.051,10,0" VerticalAlignment="Top" Click="BtnConfirm_Click"/>
            <Button x:Name="BtnCancal" Content="取消" Grid.Column="1" Margin="10,60.573,10,46.19" d:LayoutOverrides="Height" Click="BtnCancal_Click"/>
            <TextBox x:Name="TbSerivce" Height="23" Margin="10,0,10,28.9" TextWrapping="Wrap" VerticalAlignment="Bottom"/>
            <Label Content="请输入服务名称" HorizontalAlignment="Left" Margin="10,25.061,0,0" d:LayoutOverrides="HorizontalAlignment" VerticalAlignment="Top"/>
        </Grid>
    </Grid>
</Window>
